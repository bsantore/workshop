{% extends 'layout.twig' %}

{% block content %}
    <div id="main">
    {% if CONTENT %}
        <H2>{{ MENU_LABEL }}</H2>
        {% autoescape false %}
            {{ CONTENT }}
        {% endautoescape %}
    {% else %}
        <H2>Page Not Found</H2>
    {% endif %}
    </div>    
{% endblock %}